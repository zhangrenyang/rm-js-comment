{
	"name": "rm-js-comment",
	"displayName": "rm-js-comment",
	"description": "rm-js-comment",
	"keywords": [
		"rm",
		"vscode",
		"plugin",
		"comments"
	],
	"version": "1.0.3",
	"publisher": "zhang-renyang",
	"icon": "images/icon.png",
	"repository": "https://github.com/zhangrenyang/rm-js-comment",
	"engines": {
		"vscode": "^1.49.0"
	},
	"categories": [
		"Formatters"
	],
	"activationEvents": [
		"onCommand:extension.removeJsComment"
	],
	"main": "./extension.js",
	"contributes": {
		"configuration": {
			"title": "rm-js-comment",
			"properties": {
				"rm-js-comment.replacer": {
					"type": "object",
					"default": [],
					"description": "设置替换文本"
				}
			}
		},
		"commands": [
			{
				"command": "extension.removeJsComment",
				"title": "rm-js-comment"
			}
		],
		"keybindings": [
			{
				"command": "extension.removeJsComment",
				"key": "ctrl+f10",
				"mac": "cmd+f10",
				"when": "editorTextFocus"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "editorFocus",
					"command": "extension.removeJsComment",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"@types/vscode": "^1.49.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"dependencies": {
		"@babel/core": "^7.18.10"
	}
}